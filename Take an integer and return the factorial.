import java.util.Scanner;

public class Factorial {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt(); 
        long fact = 1;
        if(n>0 && n<=100){
        for (int i = 1; i <= n; i++) {
            fact = fact * i;
        }
      }
      else{
        System.out.println("Invalid input");
      }
        System.out.println(fact);
    }
}
