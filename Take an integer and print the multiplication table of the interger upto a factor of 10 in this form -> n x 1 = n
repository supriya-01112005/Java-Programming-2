import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner sc=new Scanner(System.in);
      int n=sc.nextInt();
       if(n>0 && n<=100)
        {
            for(int i=1;i<=10;i++)
            {
                System.out.println(n + " x " + i + " = " +(n*i));
            }
        }
        else 
        {
            System.out.println("Invalid Input");
        }
    }
}
